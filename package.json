{
  "name": "component-font-awesome",
  "version": "0.0.1",
  "description": "package font-awesome component in react",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack",
    "lib": "./node_modules/.bin/babel src --copy-files --source-maps --extensions .es6,.es,.js --out-dir lib",
    "dev": "rimraf lib && babel src --watch --copy-files --source-maps --extensions .es6,.es,.js --out-dir lib",
    "compile": "rimraf lib && babel src --copy-files --source-maps --extensions .es6,.es,.js --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aus0049/component-font-awesome.git"
  },
  "keywords": [
    "react",
    "font-awesome"
  ],
  "author": "Aus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aus0049/component-font-awesome/issues"
  },
  "homepage": "https://github.com/Aus0049/component-font-awesome#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "postcss-loader": "^2.0.9",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
